{
  "devCommand": "uvicorn vercel:app --host 0.0.0.0 --port 3000",
  "builds": [
      {
          "src": "platform/agent_platform/__main__.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "platform/agent_platform/__main__.py"
      }
  ]
}
